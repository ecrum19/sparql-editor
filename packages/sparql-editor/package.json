{
  "name": "@sib-swiss/sparql-editor",
  "version": "0.2.4",
  "description": "A standard web component to easily deploy a user-friendly SPARQL query editor for one or more endpoints. Built on the popular YASGUI editor, it provides context-aware autocomplete for classes and predicates based on the content of the endpoints.",
  "license": "MIT",
  "author": {
    "name": "Vincent Emonet",
    "email": "vincent.emonet@gmail.com"
  },
  "type": "module",
  "main": "dist/sparql-editor.umd.cjs",
  "module": "dist/sparql-editor.js",
  "types": "dist/src/sparql-editor.d.ts",
  "files": [
    "dist/*",
    "../../README.md",
    "../../LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest run",
    "upgrade": "npx npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@comunica/query-sparql": "^4.1.0",
    "@sib-swiss/sparql-overview": "*",
    "@zazuko/yasgui": "^4.5.0",
    "highlight.js": "^11.10.0",
    "mermaid": "^11.4.0",
    "sparqljs": "^3.7.3"
  },
  "keywords": [
    "SPARQL",
    "editor",
    "YASGUI",
    "query",
    "semantic web",
    "RDF",
    "Linked Data"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sib-swiss/sparql-editor.git"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
